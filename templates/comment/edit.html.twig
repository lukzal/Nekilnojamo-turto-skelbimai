{% extends 'base.html.twig' %}

{% block title %}Redaguoti Komentarą{% endblock %}

{% block body %}
<div class="container">
    <h1>Redaguoti Komentarą</h1>

<form role="form" class="row">

  <div class="form-group col-lg-12">
    <textarea type="text" name="message" rows="5" class="form-control textarea input-normal" placeholder="Komentaras" required></textarea>
  </div>


  <div class="form-group col-lg-12">
  <button type="submit" class="btn btn-primary btn-block">Pateikti</button>
  </div>
</form>
    </div>
{% endblock %}
